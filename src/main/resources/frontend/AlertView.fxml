<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.frauddetector.frontend.controllers.AlertViewController">
    <children>
        <TableView fx:id="alertTable" layoutX="10" layoutY="10" prefWidth="780"
                   prefHeight="580">
            <columns>
                <TableColumn fx:id="ruleColumn" text="Rule" prefWidth="200"/>
                <TableColumn fx:id="detailsColumn" text="Details" prefWidth="400"/>
                <TableColumn fx:id="timestampColumn" text="Timestamp" prefWidth="180"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
